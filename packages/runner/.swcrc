{
  "$schema": "http://json.schemastore.org/swcrc",
  "isModule": true,
  "jsc": {
    "target": "es2022",
    "parser": {
      "syntax": "typescript",
      "dynamicImport": false
    }
  }, 
  "module": {
    "type": "es6",
    "strict": true,
    "resolveFully": true,
    "noInterop": true,
    "ignoreDynamic": true, 
    "importInterop": "none"
  }
}