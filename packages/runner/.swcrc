{
  "$schema": "http://json.schemastore.org/swcrc",
  "isModule": true,
  "jsc": {
    "target": "es2022",
    "parser": {
      "syntax": "typescript",
      "dynamicImport": false
    }
  }, 
  "module": {
    "type": "es6",
    "strict": false,
    "ignoreDynamic": true,
    "strictMode": true,
    "lazy": false,
    "noInterop": true
  }
}